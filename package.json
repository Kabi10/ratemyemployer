  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "update-types": "npx supabase gen types typescript --project-id stvmsmqnrwiifwthlppy > src/types/supabase.ts",
    "cli": "cross-env NODE_OPTIONS=\"--import tsx\" node scripts/cli.ts",
    "verify": "cross-env NODE_OPTIONS=\"--import tsx\" node scripts/verify-build.ts",
    "verify:ci": "cross-env CI=true NODE_OPTIONS=\"--import tsx\" node scripts/verify-build.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:setup": "cross-env NODE_OPTIONS=\"--import tsx\" node scripts/test/setup.ts",
    "test:fix": "cross-env NODE_OPTIONS=\"--import tsx\" node scripts/test/fix.ts",
    "test:all": "npm run test:setup && npm run test:fix && npm run test",
    "fix:use-client": "cross-env NODE_OPTIONS=\"--import tsx\" node scripts/fix-use-client.ts"
  },
