{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [
    { "source": "/shame", "destination": "/shame" },
    { "source": "/shame/(.*)", "destination": "/shame/$1" }
  ]
} 